*
  -webkit-box-sizing: border-box
  -moz-box-sizing: border-box
  box-sizing: border-box

*:before, *:after
  -webkit-box-sizing: border-box
  -moz-box-sizing: border-box
  box-sizing: border-box

.clearfix:after {
  content: ".";
  display: block;
  clear: both;
  overflow: hidden;
  height: 0;
  visibility: visible
}
.fl
  float: left
.fr
  float: right

.mt15
  margin-top: 15px
.mt20
  margin-top: 20px
.mr20
  margin-right: 20px

// componets common
.search-bar
  line-height: 20px
  display: inline-block
  padding: 5px 0
  vertical-align: top;
  .bar-name
    font-size: 14px
    float: left
    line-height: 1
    padding: 8px 12px 8px 0
    width: 75px;
    text-align: right;
  .validator-cpt
    width: 200px;
    margin-left: 75px;
    .validator-cpt-tip
      font-size: 12px;
      box-sizing: border-box;
      line-height: 22px;
      text-indent: 0px;
      margin-top: 5px;
      word-break: break-word;
      padding: 0 5px
      &.error
        background-color: #ffe6e6;
        color: #e74c3c;
        border: 1px solid #f4d9dc;
.filter-box
  padding: 15px 0 10px 20px;
  border: 1px solid #e6e6e6;
  background: #fff;
  margin-bottom: 30px;
  .btn-right-wrap
    display : inline-block
    float : right
    height: 40px;
    line-height: 40px
    margin-right : 30px
    .el-button
      min-width : 80px
.table-header
  margin-bottom : 10px
  .el-button
    min-width : 60px

.el-table::after, .el-table::before
  z-index : 3

.el-pagination
